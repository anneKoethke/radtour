legend-h3 {\r\n  background-color: #006699;\r\n  color: white;\r\n  padding: .5em;\r\n  cursor: pointer;\r\n  margin: .1em 0;\r\n}\r\n#legend-h3:hover {\r\n  background-color: #6699cc;\r\n}\r\n#MZ-lightblue {\r\n  background-color: #a7ccdc;\r\n}\r\n#MZ-midblue {\r\n  background-color: #6699cc;\r\n}\r\n#MZ-darkblue {\r\n  background-color: #006699;\r\n}\r\n#MZ-lightgray {\r\n  background-color: #8e8e8e;\r\n}\r\n#MZ-darkgray {\r\n  background-color: #3e3e3e;\r\n}\r\n#legend-inner-container {\r\n  margin: .1em 0 1em .1em;\r\n}\r\n\r\n#info-row {\r\n  margin: 0 0 .5em 0;\r\n}\r\n#info-row > p {\r\n  margin: 0;\r\n}\r\n.legend-rows {\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr 1fr 1fr 1fr;\r\n  grid-gap: 1em;\r\n}\r\n.legend-rows > div {\r\n  display: grid;\r\n  grid-template-columns: 1fr 9fr;\r\n  grid-gap: .5em;\r\n}\r\n.legend-p {\r\n  margin: 0;\r\n}\r\n\r\n/* USER INTERACTION */\r\n.hidden {\r\n  display: none !important;\r\n}\r\nhr {\r\n  max-width: 870px;\r\n  margin: .5em 0;\r\n}\r\n#user-info {\r\n  max-width: 870px;\r\n  margin: .1em 0;\r\n}\r\n#user-info-h3 {\r\n  background-color: #8e8e8e;\r\n  color: white;\r\n  padding: .5em;\r\n  cursor: pointer;\r\n  margin: .1em 0;\r\n  /* added to solve scroll prob on safari with child-div #user-info-inner-container: START\r\n  -webkit-overflow-scrolling: touch;\r\n  overflow-y: scroll;\r\n  -> macht nur hässliches Scroll-Ding.. \r\n  added to solve scroll prob on safari with child-div #user-info-inner-container: END */\r\n}\r\n#user-info-h3:hover {\r\n  background-color: #3e3e3e;\r\n}\r\n\r\n/* POPUP */\r\n\r\n.popup {\r\n  width: 250px;\r\n  padding: .5em 1em;\r\n  height: auto;\r\n  background-color: #006699;\r\n  color: white;\r\n  /* für ungefähr mittige Anordnung des Popup - prob bei großen Bildschirmen, weil nicht auf 780px-contaienr beschränkt */\r\n  position: fixed;  \r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  \r\n}\r\n.popup-row {\r\n  display: grid; \r\n  grid-template-columns: 7fr 1fr;\r\n}\r\n.popup-title {\r\n  margin: .3em 0;\r\n}\r\n.close {\r\n  margin: auto 0;\r\n  background-color: #3e3e3e;\r\n  color: white;\r\n  border: 1px solid white;\r\n  text-align: center;\r\n  cursor: pointer;\r\n  font-style: inherit;\r\n  font-weight: bold;\r\n  font-size: 1.5em;\r\n}\r\n.close:hover {\r\n  background-color: #8e8e8e;\r\n  color: #3e3e3e !important;\r\n}\r\n.popup > p {\r\n  color: white;\r\n}\r\n.adr {\r\n  font-style: inherit;\r\n}\r\n.popup-link-row {\r\n  display: grid;\r\n  grid-template-columns: 2fr 2fr 2fr;\r\n  grid-gap: .2em;\r\n}\r\n.popup-link {\r\n  color: white !important;\r\n  text-decoration: none;\r\n  background-color: #3e3e3e;\r\n  border: 1px solid white;\r\n  padding: .1em .5em .3em .5em;\r\n  text-align: center;\r\n  cursor: pointer;\r\n  font-style: inherit;\r\n  font-weight: bold;\r\n}\r\n.popup-link:hover {\r\n  background-color: #8e8e8e;\r\n  color: #3e3e3e !important;\r\n}\r\n.popup-single-item {\r\n  display: grid;\r\n  grid-template-columns: 1fr;\r\n}\r\n.link-info {\r\n  font-size: .7em;\r\n  margin: 0;\r\n}\r\n\r\n/* MEDIA-QUERIES */\r\n@media only screen and (max-width: 840px) {\r\n  .legend-rows{\r\n    grid-template-columns: 1fr 1fr 1fr;\r\n    grid-gap: .5em;\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 520px) {\r\n  .legend-rows{\r\n    grid-template-columns: 1fr 1fr;\r\n    grid-gap: .1em;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"ffcd82842bb7ef30d969e5b094751a7e","cacheData":{"env":{}}}