 url('../font/national2/National2Web-Bold.woff2') format('woff2'); /* Super Modern Browsers */\r\n  src: url('../font/national2/National2Web-Regular.woff') format('woff'); /* Pretty Modern Browsers */\r\n  font-weight: 400;\r\n  font-style: normal;\r\n}\r\nbody {\r\n  font-family: 'National2WebRegular', serif;\r\n}\r\np, a {\r\n  font-style: italic;\r\n  font-size: .9em;\r\n  color: #8e8e8e;\r\n  margin: .4em 0;\r\n}\r\na:link, a:visited, a:active, a:hover {\r\n  color: #8e8e8e;\r\n}\r\nspan {\r\n  font-weight: bold; \r\n}\r\n\r\n/* class and id based */\r\n/* MAP */\r\n#map-container {\r\n  max-width: 870px;\r\n}\r\n#map {\r\n  max-width: 869px;\r\n  height: 350px;\r\n  background-color: rgba(0, 0, 0, .5)\r\n}\r\n/* canvas is generated by OL inside #map*/\r\ncanvas.ol-unselectable {\r\n  opacity: .8;\r\n}\r\n#hidden-div {\r\n  display: none;\r\n}\r\n.marker {\r\n  width: 9px;\r\n  height: 9px;\r\n  border-radius: 20px;\r\n  background-color: #3e3e3e;\r\n  cursor: help;\r\n}\r\n.marker:hover {\r\n  background-color: orange !important;\r\n  border-color: white;\r\n}\r\n.perma-marker {\r\n  width: 9px;\r\n  height: 9px;\r\n  border-radius: 10px;\r\n  background-color: red;\r\n}\r\n.marker-legend {\r\n  width: 9px;\r\n  height: 9px;\r\n  border-radius: 10px;\r\n  background-color: #3e3e3e;\r\n  margin: 5px 0;\r\n}\r\n#legend {\r\n  margin: .1em 0;\r\n}\r\n#legend-h3 {\r\n  background-color: #1d3a4d;\r\n  color: white;\r\n  padding: .5em;\r\n  margin: .1em 0;\r\n}\r\n#MZ-lightblue {\r\n  background-color: #96B8C6; /* actual lightblue: #a7ccdc; made darker for visualization purposes */\r\n}\r\n#MZ-midblue {\r\n  background-color: #6699cc;\r\n}\r\n#MZ-darkblue {\r\n  background-color: #006699; /* new darkblue #1d3a4d */\r\n}\r\n#MZ-new_darkblue {\r\n  background-color: #1d3a4d;\r\n}\r\n#MZ-lightgray {\r\n  background-color: #8e8e8e;\r\n}\r\n#MZ-darkgray {\r\n  background-color: #727272; /* actual Lightgray: #3e3e3e; made darker for visualization purposes */\r\n}\r\n#legend-inner-container {\r\n  margin: .1em 0 1em .1em;\r\n}\r\n\r\n#info-row {\r\n  margin: 0 0 .5em 0;\r\n}\r\n#info-row > p {\r\n  margin: 0;\r\n}\r\n.legend-rows {\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr 1fr 1fr 1fr;\r\n  grid-gap: 1em;\r\n}\r\n.legend-rows > div {\r\n  display: grid;\r\n  grid-template-columns: 1fr 9fr;\r\n  grid-gap: .5em;\r\n}\r\n.legend-p {\r\n  margin: 0;\r\n}\r\n\r\n/* POPUP */\r\n.popup {\r\n  width: 250px;\r\n  padding: .5em 1em;\r\n  height: auto;\r\n  background-color: #1d3a4d;\r\n  color: white;\r\n  /* für ungefähr mittige Anordnung des Popup - prob bei großen Bildschirmen, weil nicht auf 780px-contaienr beschränkt */\r\n  position: fixed;  \r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  \r\n}\r\n.popup-row {\r\n  display: grid; \r\n  grid-template-columns: 7fr 1fr;\r\n}\r\n.popup-title {\r\n  margin: .3em 0;\r\n}\r\n.close {\r\n  margin: auto 0;\r\n  background-color: #069;\r\n  color: white;\r\n  border: 1px solid white;\r\n  text-align: center;\r\n  cursor: pointer;\r\n  font-style: inherit;\r\n  font-weight: bold;\r\n  font-size: 1.5em;\r\n}\r\n.close:hover {\r\n  background-color: #8e8e8e;\r\n  color: #069 !important;\r\n}\r\n.popup > p {\r\n  color: white;\r\n}\r\n.bildnachweis {\r\n  font-size: smaller;\r\n  margin: 0;\r\n  text-align: end;\r\n}\r\n.adr {\r\n  font-style: inherit;\r\n}\r\n.popup-link-row {\r\n  display: grid;\r\n  grid-template-columns: 2fr 2fr 2fr;\r\n  grid-gap: .2em;\r\n}\r\n.popup-link {\r\n  color: white !important;\r\n  text-decoration: none;\r\n  background-color: #069;\r\n  border: 1px solid white;\r\n  padding: .1em .5em .3em .5em;\r\n  text-align: center;\r\n  cursor: pointer;\r\n  font-style: inherit;\r\n  font-weight: bold;\r\n}\r\n.popup-link:hover {\r\n  background-color: #8e8e8e;\r\n  color: #069 !important;\r\n}\r\n.popup-single-item {\r\n  display: grid;\r\n  grid-template-columns: 1fr;\r\n}\r\n.link-info {\r\n  font-size: .7em;\r\n  margin: 0;\r\n}\r\n\r\n/* SEARCH BUTTON & INPUT */\r\n.search-control {\r\n  display: grid;\r\n  grid-template-columns: 1fr 6fr;\r\n}\r\n\r\n/* AUTOCOMPLETE: https://www.w3schools.com/howto/tryit.asp?filename=tryhow_js_autocomplete */\r\n.autocomplete {\r\n  position: relative;\r\n  display: inline-block;\r\n}\r\n\r\n.autocomplete-items {\r\n  position: absolute;\r\n  border: 1px solid #d4d4d4;\r\n  border-bottom: none;\r\n  border-top: none;\r\n  z-index: 99;\r\n  /*position the autocomplete items to be the same width as the container:*/\r\n  top: 100%;\r\n  left: 0;\r\n  right: 0;\r\n  /* make results scrollable */\r\n  max-height: 220px;\r\n  overflow: auto;\r\n}\r\n\r\n.autocomplete-items div {\r\n  padding: 10px;\r\n  cursor: pointer;\r\n  background-color: #fff; \r\n  border-bottom: 1px solid #d4d4d4; \r\n}\r\n\r\n/*when hovering an item:*/\r\n.autocomplete-items div:hover {\r\n  background-color: #e9e9e9; \r\n}\r\n\r\n/*when navigating through the items using the arrow keys:*/\r\n.autocomplete-active {\r\n  background-color: DodgerBlue !important; \r\n  color: #ffffff; \r\n}\r\n\r\n/* MEDIA-QUERIES */\r\n@media only screen and (max-width: 840px) {\r\n  .legend-rows{\r\n    grid-template-columns: 1fr 1fr 1fr;\r\n    grid-gap: .5em;\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 520px) {\r\n  .legend-rows{\r\n    grid-template-columns: 1fr 1fr;\r\n    grid-gap: .1em;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"b0d16bbb4b7763dc9e5773e929c90ac3","cacheData":{"env":{}}}